all:	sources.200MB proteins.200MB dna.200MB english.200MB dblp.xml.200MB

sources.200MB:	
	curl -O http://pizzachili.dcc.uchile.cl/texts/code/sources.200MB.gz
	gunzip sources.200MB.gz

proteins.200MB:	
	curl -O http://pizzachili.dcc.uchile.cl/texts/protein/proteins.200MB.gz
	gunzip proteins.200MB.gz

dna.200MB:
	curl -O http://pizzachili.dcc.uchile.cl/texts/dna/dna.200MB.gz
	gunzip dna.200MB.gz

english.200MB:
	curl -O http://pizzachili.dcc.uchile.cl/texts/nlang/english.200MB.gz
	gunzip english.200MB.gz

dblp.xml.200MB:
	curl -O http://pizzachili.dcc.uchile.cl/texts/xml/dblp.xml.200MB.gz
	gunzip dblp.xml.200MB.gz
